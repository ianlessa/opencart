<div class="tab-pane alert alert-info" id="boletoTab">
    <div class="row">
        <h3 class="col-sm-12">{{ boleto.title }}</h3>
        <div class="col-sm-7">
            <blockquote>
                <h8>{{ boleto.instructions }}</h8>
            </blockquote>
        </div>
        <div class="col-sm-2">
            <img src="image/catalog/mundipagg/boleto.png" title="Boleto" alt="Boleto" width="75">
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <button
                    id="generateBoletoBtn"
                    type="submit"
                    class="btn btn-primary float-to-right generate"
                    id="submit-payment-button"
                    onclick="generate();"
            >Clique aqui para gerar seu boleto</button>
        </div>
    </div>
</div>

